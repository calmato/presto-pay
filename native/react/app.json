{
  "expo": {
    "name": "PrestoPay",
    "slug": "react",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/presto-pay.png",
      "resizeMode": "contain",
      "backgroundColor": "#2BACFC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "work.calmato.presto-pay"
    },
    "android": {
      "package": "work.calmato.presto_pay",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
